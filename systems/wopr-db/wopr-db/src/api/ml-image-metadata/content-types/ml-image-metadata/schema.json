{
  "kind": "collectionType",
  "collectionName": "ml_image_metadatas",
  "info": {
    "singularName": "ml-image-metadata",
    "pluralName": "ml-image-metadatas",
    "displayName": "mlImageMetadata"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "filename": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "uid": {
      "type": "uid",
      "required": true
    },
    "createTime": {
      "type": "datetime",
      "required": true
    },
    "updateTime": {
      "type": "datetime"
    },
    "game": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::game.game"
    },
    "piece": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::piece.piece"
    },
    "objectRotation": {
      "type": "integer"
    },
    "objectPosition": {
      "type": "string",
      "default": "random",
      "required": true
    },
    "colorTemp": {
      "type": "string"
    },
    "lightIntensity": {
      "type": "string"
    }
  }
}
